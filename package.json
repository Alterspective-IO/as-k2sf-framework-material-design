{
  "name": "@alterspective-io/as-k2sf-framework-material-design",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/alterspective-io/as-k2sf-framework-material-design.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "description": "Collection of material design custom components",
  "main": "/dist/main.alterspective.materialdesignforK2.js.js",
  "types": "/build/src/index.d.ts",
  "scripts": {
      "doc": "typedoc --entryPoints src --out docs",
    "expose": "ngrok http --domain=sharedo.alterspective.io 9000",
    "deploy": "ts-node-esm uploadToAzure.mts",
    "clean-files": "concurrently \"rimraf build\" \"rimraf dist\"",
    "build": "npm run clean-files && webpack",
    "copy-files": "copyfiles -u 1 src/**/*.html Test/**/*.html src/**/*.css src/**/*.jpg  src/**/*.scss build/",
    "start": "concurrently \"rimraf build\" \"rimraf dist\" && npm run copy-files && webpack serve"
  },
  "keywords": [
    "K2",
    "UX",
    "Simplied"
  ],
  "author": "Igor Jericevich",
  "license": "ISC",
