{


  "compilerOptions": {
    "target": "es6",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "allowJs": true,
    "sourceMap": true,
    "outDir": "build",
    "downlevelIteration": true,
    "skipLibCheck": true,
    "strict": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "lib" :[
      "dom",
      "es5",
      "es6",
      "DOM.Iterable"
    ],
    "paths": {
      "timers": [
        "./node_modules/timers-browserify"
      ],
      "stream": [
        "./node_modules/stream-browserify"
      ],
      "@alterspective-io/as-k2sf-framework":["./framework/src/"],
      // "@alterspective-io/as-framework-material-design/dist/types/components":["/Users/igorsharedo/Documents/GitHub/as-framework-material-design/dist/components"],
    }
  },
  "include":
  [
    "framework/**/*", "src/**/*","Test","./declaration.d.ts" ,"./Globals.d.ts"
, "debouncer.mts", "DefferedPromise.mts"  ]
  }