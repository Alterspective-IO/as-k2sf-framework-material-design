{


  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitOverride": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "allowJs": true,
    "sourceMap": true,
    "outDir": "build",
    "downlevelIteration": true,
    "strict": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "lib" :[
      "dom",
      "es5",
      "es6",
      "DOM.Iterable"
    ],
    "paths": {
      "timers": [
        "./node_modules/timers-browserify"
      ],
      "stream": [
        "./node_modules/stream-browserify"
      ],
      "@alterspective-io/as-k2sf-framework":["./framework/src/"],
    }
  },
  "include":
  [
    "framework/**/*", "src/**/*","Test","./declaration.d.ts" ,"./Globals.d.ts"
, "debouncer.mts", "DefferedPromise.mts"  ]
  }